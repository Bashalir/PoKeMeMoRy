    "sourceType": "module"
  },

  "extends": ["plugin:prettier/recommended", "eslint-config-prettier"],

  "plugins": ["prettier"],

  "rules": {
    "strict": ["error", "never"]
  },

  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
